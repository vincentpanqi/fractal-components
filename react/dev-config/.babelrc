{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-1",
    "flow"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "async-to-promises",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}